    "version": "4.3.0",
    "type": "craft-plugin",
    "homepage": "https://putyourlightson.com/plugins/sherlock",
    "license": "proprietary",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "security",
        "scanner",

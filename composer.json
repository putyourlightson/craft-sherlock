        "codeception/module-asserts": "^1.3.1",
        "codeception/module-yii2": "^1.1.5",
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "yiisoft/yii2-redis": "^2.0",
        "vlucas/phpdotenv": "^5.4.1"
